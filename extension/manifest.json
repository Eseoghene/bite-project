{
  "name": "BITE-Playground (Dev)",
  "version": "2.0.1.13",
  "manifest_version": 2,
  "description": "Open source version of BITE. (Dev)",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": {"19": "imgs/logo-19x19.png"},
    "default_title": "BITE",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "imgs/logo-48x48.png",
    "128": "imgs/logo-128x128.png"
  },
  "background": {"page": "background.html"},
  "content_scripts": [
    {
      "js": [ "getactioninfo_script.js" ],
      "matches": [ "*://*/*" ],
      "all_frames": true
    }, {
      "js": [ "content_script.js" ],
      "matches": [ "*://*/*" ],
      "exclude_globs": [ "*://code.google.com/*" ]
    }
  ],
  "content_security_policy": "script-src 'self' https://d1n0x3qji82z53.cloudfront.net https://www.google-analytics.com; object-src 'self'",
  "incognito": "spanning",
  "permissions": [
    "tabs",
    "<all_urls>",
    "management",
    "unlimitedStorage"
  ]
}

