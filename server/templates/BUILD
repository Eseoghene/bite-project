# All rules between these lines generated by GenJsDeps, please do not edit.

filegroup(name='test_files',
          srcs=glob(['*_test.html']))

filegroup(name='test_files_recursive',
          srcs=[':test_files'])

# All rules between these lines generated by GenJsDeps, please do not edit.
#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@#@


filegroup(name = 'opensource_files',
          srcs = glob(['*'], exclude=['*~', '**', 'site_compat.html']))


filegroup(name = 'templates',
          srcs = glob(['*'], exclude = ['*~']))
